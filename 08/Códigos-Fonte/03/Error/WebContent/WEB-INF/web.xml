    <servlet-class>servlet.TesteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TesteServlet</servlet-name>
    <url-pattern>/Teste</url-pattern>
  </servlet-mapping>
</web-app>